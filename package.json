{
	"name": "@sevan-aio/nike-release-checker-terminal",
	"description": "Simple terminal Nike stock checker (50+ countries)",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"imports": {
		"#snkrs-sdk": "./snkrs-sdk/index.ts",
		"#snkrs-sdk/mocks": "./snkrs-sdk/mocks/node.ts"
	},
	"scripts": {
		"start": "tsx --experimental-webstorage --localstorage-file=local.db --no-warnings src/devEntry.ts",
		"start:debug": "tsx --inspect-brk --experimental-webstorage --localstorage-file=local.db --no-warnings src/devEntry.ts",
		"start:watch": "tsx --watch-path=./src --experimental-webstorage --localstorage-file=local.db --no-warnings src/devEntry.ts",
		"build": "rspack build",
		"test": "node --experimental-strip-types --no-warnings --test",
		"lint:ts": "tsc --noEmit"
	},
	"keywords": [
		"api",
		"snkrs",
		"terminal",
		"stock",
		"nike"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@nanostores/persistent": "1.0.0",
		"@nanostores/react": "1.0.0",
		"@nanostores/router": "1.0.0",
		"figures": "6.1.0",
		"ink": "5.2.0",
		"ink-select-input": "6.0.0",
		"ink-spinner": "5.0.0",
		"nanostores": "1.0.1",
		"radash": "12.1.0",
		"react": "18.3.1",
		"terminal-image": "3.1.1"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "4.4.1",
		"@rspack/cli": "^1.3.5",
		"@rspack/core": "^1.3.5",
		"@types/node": "22.14.1",
		"@types/react": "18.3.19",
		"msw": "2.7.4",
		"prettier": "3.5.3",
		"tsx": "4.19.3",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": ">=22.7"
	}
}
